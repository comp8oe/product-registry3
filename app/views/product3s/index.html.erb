<p id="notice"><%= notice %></p>

<h1>Product3s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Vendor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product3s.each do |product3| %>
      <tr>
        <td><%= product3.name %></td>
        <td><%= product3.price %></td>
        <td><%= product3.vendor %></td>
        <td><%= link_to 'Show', product3 %></td>
        <td><%= link_to 'Edit', edit_product3_path(product3) %></td>
        <td><%= link_to 'Destroy', product3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product3', new_product3_path %>
